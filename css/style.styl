    position: relative;
    input
        display: block
        width: 100%
        height: 40px;
        border: 2px solid #bcc0c3;
        border-right: none;
        border-radius: 3px 0 0 3px;
        padding: 0 160px 0 12px;
    button
        width: 147px;
        height: 40px;
        background: #26598c url("../assets/img/search-icn.png") no-repeat 116px 10px;
        border: none;
        border-radius: 0 3px 3px 0;
        color: #fff
        text-align: left
        padding-left 23px
        position: absolute;
        top 0
        right 0
        &:after
            content: ''
            display: block;
            width: 1px
            height: 27px
            background: #204a75
            position: absolute;
            top 7px
            right: 40px
    .bbl
        margin-top 55px
        text-align center
        div
            font-size: 16px
            font-weight: bold
            text-transform uppercase
            margin-bottom 25px
        ul
            li
                display: inline-block;
                margin: 0 2px 10px
                a
                    display: block
                    width: 32px;
                    height: 32px;
                    line-height: 32px
                    text-align: center
                    background: #f6f8fa
                    border: 1px solid #dddfe1
                    border-radius: 3px;
                    font-family: 'GothaProReg';
                    font-size: 15px
                    color: #323232
@media (max-width: 649px){
    .search-form input{
        border: 2px solid #bcc0c3
        border-radius: 3px
        padding: 0 12px
    }
    .search-form button{
        position: relative;
        width: 100%;
        margin-top 15px
        background: #26598c url("../assets/img/search-icn.png") no-repeat 98% 10px;
        border-radius: 3px
    }
}
.newOrder-page
    .small-banners
        margin: 35px 0
        .btn
            line-height: 1.3
            padding: 10px 15px
            font-size: 20px
            img
                position: absolute;
                left: 15px
            b
                display: block
            &.call-btn
                b
                    font-size: 25px
                img
                    top: 50%
                    margin-top -16px
            &.presctiption-btn
                padding: 16px 15px
                font-size: 17px

@media (max-width: 569px){
    .newOrder-page .small-banners .btn{
        height: auto;
        font-size: 15px
    }
    .newOrder-page .small-banners .btn.presctiption-btn{
        font-size: 14px
    }
    .newOrder-page .small-banners .btn.presctiption-btn b{
        font-size: 16px
    }
}
.caregory-product
    .slide-block
        display: inline-block;
        vertical-align: top;
        width: 49.5%
        margin-bottom: 12px
        &:nth-child(2n)
            margin-left: 1%
        .title
            background: #f6f8fa
            border: 1px solid #dddddd;
            height: 45px
            line-height: 45px
            padding: 0 40px 0 15px
            cursor: pointer;
            position: relative;
            -webkit-user-select: none;
            -moz-user-select: none;
            -user-select: none;
            white-space: nowrap;
            overflow: hidden;
            &:after
                content: ''
                width: 0;
                height: 0;
                border-style: solid;
                border-width: 9px 8px 0 8px;
                border-color: #2bbd8e transparent transparent transparent;
                position: absolute;
                top 18px
                right 10px
        .inner
            display: none
            padding: 15px
            ul
                li
                    display: inline-block;
                    width: 49%
                    a
                        font-family: 'GothaProReg';
                        font-size: 15px
                        line-height: 29px
        &.open
            .title:after
                border-width: 0 8px 9px 8px;
                border-color: transparent transparent #2bbd8e transparent;
@media (max-width: 759px){
    .caregory-product .slide-block{
        display: block;
        width: auto;
    }
    .caregory-product .slide-block:nth-child(2n){
        margin-left 0
    }
}
.cantFind
    margin: 45px 0
    text-align center
    font-size: 18px;
    .title
        font-family: 'GothaProBold';
        font-size: 18px;
        color: #26598c
        margin-bottom: 25px
    .contacts
        line-height: 32px
        text-align: left
        .td:nth-child(1)
            text-align: right
            width: 48%
        .td:nth-child(2)
            text-align: center
            width: 4%
        img
            margin-top -4px
    .small-banners .btn
        margin-bottom 0
@media (max-width: 991px){
    .cantFind .contacts .td:nth-child(1){
        width: 47%;
    }
    .cantFind .contacts .td:nth-child(2){
        width: 6%;
    }
}
@media (max-width: 649px){
    .cantFind .contacts .tr{
        display: block
    }
    .cantFind .contacts .td:nth-child(1){
        width: 100%;
        display: block;
        text-align: left
    }
    .cantFind .contacts .td:nth-child(2){
        width: 40px;
    }
}
@media (max-width: 345px){
    .cantFind{
        font-size: 16px
    }
}
/* ==========================================================================
   Product page
   ========================================================================== */
.product-page
    h1
        margin: 50px 0
        text-align center
        font-family: 'GothaProBold';
        font-size: 17px
        color: #9d9e9e
        text-transform uppercase
        span
            font-size: 26px
            color: #21a0a0
    .product
        background: #f6f8fa
        border: 1px solid #dddfe1
        border-radius: 3px
        padding: 0 22px 22px
        margin-bottom: 15px
        &.generic .category
            color: #21a0a0
        &.generic .product-name
            color: #21a0a0
        .category
            padding: 10px 5px
            text-align center
            font-family: 'GothaProBold';
            font-size: 12px
            color: #26598c
            border-bottom: 1px solid #dddfe1
            text-transform uppercase
            margin-bottom 20px
        .product-name
            font-size: 20px
            color: #26598c
            margin-bottom 20px
        .manufactured,
        .country
            font-family: 'GothaProReg';
            font-size: 13px
            color: #040404
            line-height: 1.2
        .prescription
            font-family: 'GothaProReg';
            font-size: 10px
            color: #e64b66
            float right
            margin-top -15px
        .table
            margin-top 20px
            background: #fff
            border: 1px solid #a7a9ac
            margin-bottom 0
            .tr:first-child
                .td
                    font-family: 'GothaProReg';
                    font-size: 12px
                    color: #828282
                    text-transform uppercase
                    height: 34px
            .td
                border-bottom: 1px solid #dde0e4
                vertical-align: middle;
                height: 58px
                font-family: 'GothaProReg';
                font-size: 15px
                color: #040404
                span
                    display: block;
                    width: 21px
                    height: 21px
                    background: url(../assets/img/check.png) no-repeat;
                    margin: 0 auto
            .td:first-child
                width: 38px
            .td:last-child
                width: 120px
                .btn
                    display: none
                    width: 120px
                    height: 57px;
                    line-height: 57px;
                    border-radius: 0;
                    font-size: 16px
                    text-transform none
            .tr+.tr:hover .td
                background: #daefef
                span
                    background-position: bottom
                .btn
                    display: block
    .product-info
        margin-top 30px
        .slide-box
            border-bottom: 1px solid #dddfe1
            .slide-box_title
                padding: 18px 0
                font-family: 'GothaProBold';
                font-size: 19px
                color: #21a0a0
                text-transform uppercase
                cursor: pointer;
                i
                    display: inline-block;
                    vertical-align: middle
                    width: 0;
                    height: 0;
                    border-style: solid;
                    border-width: 0 5px 6px 5px;
                    border-color: transparent transparent #21a0a0 transparent;
            &.open i
                border-width: 6px 5px 0 5px;
                border-color: #21a0a0 transparent transparent transparent;
            .slide-box_inner
                display: none
                padding: 0 0 15px
                line-height: 1.2
                font-size: 14px
        .discaimer
            border: 1px solid #5a5a5a
            border-radius: 3px
            padding: 10px 10px 0
            margin-top 30px
            font-size: 13px
@media (max-width: 991px){
    .product-page .product .table .td:last-child .btn{
        display: block;
    }
}
@media (max-width: 479px){
    .product-page .product{
        padding: 0 12px 12px;
    }
    .product-page .product .prescription{
        float none
        margin-top 5px
    }
    .product-page .product .table,
    .product-page .product .table .tr
    {
        display: block;
    }
    .product-page .product .table .tr{
        margin-bottom 15px
    }
    .product-page .product .table .tr:first-child,
    .product-page .product .table .tr .td:first-child
    {
        display: none;
    }
    .product-page .product .table .td{
        display: inline-block;
        border-bottom: none;
        height: auto;
        padding: 5px 10px;
    }
    .product-page .product .table .td:last-child{
        display: block;
        width: auto;
    }
    .product-page .product .table .td:last-child .btn{
        display: block;
        width: 100%;
        height: 40px;
        line-height: 40px;
    }
    .product-page .product .table .tr+.tr:hover .td{
        background: #fff;
    }
}
